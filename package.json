    "pre-commit": "lint-staged && npm test"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.0",
    "cookies-next": "^5.1.0",
    "i18next": "^23.16.8",
    "i18next-resources-to-backend": "^1.2.1",
    "next": "14.1.0",
    "next-i18next": "^15.4.2",
    "react": "^18",
    "react-dom": "^18",
    "react-i18next": "^14.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@biomejs/biome": "^1.9.4",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^3.0.9",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.5.0",
    "postcss": "^8.5.3",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vitest": "^3.0.9"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
